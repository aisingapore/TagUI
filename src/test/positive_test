click element

// test {} simple
{
this.echo('test');
}
{
click test
}

// test {} complex
{
click test1
click test2
this.echo('test');
click test3
}

// test {} for loop simple
for number from 1 to 5
{
click test
}

// test {} for loop complex
for number from 1 to 5
{
click test1
click test2
this.echo('test');
click test3
}

// test {} while loop simple
var number = 1;
while (number <= 5)
{
click test
}

// test {} while loop complex
while (number <= 5)
{
click test1
click test2
this.echo('test');
click test3
number++;
}

// test {} for popup step
popup about
{
click test
}
